{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ES2015",
    "moduleResolution": "node",
    "lib": ["ES2018", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true
  }
}
